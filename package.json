{
  "name": "website-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve --proxy-config proxy/proxy.local.conf.json",
    "start:dev": "ng serve --proxy-config proxy/proxy.dev.conf.json",
    "start:live": "ng serve --proxy-config proxy/proxy.live.conf.json",
    "build": "ng build --prod",
    "dev:ssr": "ng run website-frontend:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run website-frontend:server:production",
    "postinstall": "ngcc",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "karma-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test-ci": "npm run karma-ci && npm run e2e-ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/platform-server": "^10.0.9",
    "@angular/router": "^10.0.9",
    "@nguniversal/express-engine": "^10.0.2",
    "express": "^4.15.2",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular/cli": "^10.0.6",
    "@angular/compiler-cli": "^10.0.9",
    "@nguniversal/builders": "^10.0.2",
    "@types/express": "^4.17.7",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
