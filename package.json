{
  "name": "website-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve --proxy-config proxy/proxy.local.conf.json",
    "start:dev": "ng serve --proxy-config proxy/proxy.dev.conf.json",
    "start:live": "ng serve --proxy-config proxy/proxy.live.conf.json",
    "build": "ng build --prod",
    "dev:ssr": "ng run website-frontend:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run website-frontend:server:production",
    "postinstall": "ngcc",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "karma-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "test-ci": "npm run karma-ci && npm run e2e-ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/platform-server": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@nguniversal/express-engine": "^11.0.0",
    "bootstrap": "^5.0.0-alpha3",
    "express": "^4.15.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@nguniversal/builders": "^11.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.9",
    "codelyzer": "^6.0.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "~9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  }
}
